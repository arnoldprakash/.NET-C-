using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int [] arr={2,11,44,54,67};
        Array.Sort(arr);
        Array.Reverse(arr);
        Console.WriteLine("Second Highest value in Aray \n"+arr [1]);
        foreach(var result in arr)
        {
            Console.WriteLine(result+",");
        }
    }
}
