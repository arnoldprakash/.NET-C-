using System;

public class HelloWorld
{
    static void Main(string[] args)
    {
        int time =22;
        if(time>10)
        {
            Console.WriteLine("good Morning");
        }
        else if(time>20)
        {
            Console.WriteLine("good afternoon");
        }
        else
        {
            Console.WriteLine("Good evening");
        }
        Console.ReadLine();
    }
}
